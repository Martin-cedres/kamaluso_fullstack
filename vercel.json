{
  "crons": [
    {
      "path": "/api/cron/recover-abandoned-orders",
      "schedule": "0 * * * *"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "papeleriapersonalizada.uy"
        }
      ],
      "destination": "https://www.papeleriapersonalizada.uy/$1",
      "permanent": true
    }
  ]
}